@model IEnumerable<Auth.Models.douong>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Tendouong)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.dongiaban)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Anh)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dongianhap)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mota)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Trongluong)
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Tendouong)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dongiaban)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Anh)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dongianhap)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mota)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Trongluong)
            </td>
           
            <td>
                @Html.ActionLink("Edit", "SuaSanPham", new { @Madouong= item.Madouong })|
                

                @Html.ActionLink("Delete", "XoaSanPham", new { @Madouong = item.Madouong })|


            </td>
        </tr>
    }

</table>


